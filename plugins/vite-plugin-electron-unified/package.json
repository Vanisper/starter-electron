{
  "name": "vite-plugin-electron-unified",
  "version": "0.0.0",
  "description": "A unified Vite plugin for Electron development with configurable build targets",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "files": [
    "dist",
    "client.d.ts",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "bunx rimraf dist"
  },
  "peerDependencies": {
    "vite": "catalog:",
    "vite-plugin-electron": "catalog:",
    "electron-builder": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:"
  }
}
