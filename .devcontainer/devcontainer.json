{
  "name": "Bun",
  "dockerFile": "Dockerfile",
  "runArgs": [
    "--network=host",
    "--name",
    "starter-electron"
  ],
  "build": {
    // Update 'VARIANT' to pick a Bun version.
    "args": {
      "VARIANT": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Vue.volar",
        "dbaeumer.vscode-eslint",
        "EditorConfig.EditorConfig"
      ]
    }
  }
}
