{
  "name": "starter-electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": {
    "packages": [
      "plugins/*"
    ],
    "catalog": {
      "typescript": "~5.9.3",
      "vite": "npm:rolldown-vite@7.1.14",
      "@vitejs/plugin-vue": "^6.0.1",
      "electron": "^39.0.0",
      "electron-v22": "npm:electron@22",
      "vite-plugin-electron": "^0.29.0",
      "electron-builder": "^26.0.12",
      "vue": "^3.5.22",
      "@vue/tsconfig": "^0.8.1",
      "vue-tsc": "^3.1.0",
      "@types/node": "^24.6.0",
      "cross-env": "^10.1.0"
    }
  },
  "trustedDependencies": ["electron", "electron-v22"],
  "scripts": {
    "dev": "vite",
    "dev:22": "cross-env VITE_TARGET_ELECTRON=electron-v22,cjs vite",
    "build": "vue-tsc -b && vite build",
    "build:22": "vue-tsc -b && cross-env VITE_TARGET_ELECTRON=electron-v22,cjs vite build",
    "run:lib": "bun --filter './packages/*' --filter './plugins/*'",
    "dev:lib": "bun run:lib -- dev",
    "build:lib": "bun run:lib -- build",
    "clear:lib": "bun run:lib -- clear",
    "clear-all:lib": "bun run:lib -- clear-all",
    "postinstall": "bun build:lib",
    "clear-all": "bunx rimraf dist dist-electron node_modules && bun clear-all:lib"
  },
  "dependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/tsconfig": "catalog:",
    "cross-env": "catalog:",
    "electron": "catalog:",
    "electron-v22": "catalog:",
    "electron-builder": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-electron": "catalog:",
    "vue-tsc": "catalog:",
    "vite-plugin-electron-unified": "workspace:*"
  },
  "overrides": {
    "vite": "catalog:"
  }
}
